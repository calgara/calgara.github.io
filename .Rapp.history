votes <- merge(vote,ev,by=c("state","time","party"), all=TRUE)#
votes <- merge(votes,twoparty_vote,by=c("state","time","party"), all=TRUE)#
votes <- merge(votes,twoparty_evvote,by=c("state","time","party"), all=TRUE)#
