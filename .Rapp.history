x$label <- paste(x$lastname,x$state_abbrev)#
x$party_code <- ifelse(x$party_code %in% 328, 100, x$party_code)#
x$party_code <- factor(x$party_code)#
#
myColors <- c("blue","red")#
names(myColors) <- levels(x$party_code)#
